<ul class="account-nav">
  {% for link in linklists.account-menu.links %}
    <li>
      <a href="{{ link.url }}">
        {% if link.title == "Home" %}
          {{ 'Home_white.png' | file_img_url | img_tag }}
        {% elsif link.title == "My Account" %}
          {{ 'account_white.png' | file_img_url | img_tag }}
        {% elsif link.title == "Shopping Cart" %}
          {{ 'cart_white.png' | file_img_url | img_tag }}
        {% elsif link.title == "Checkout" %}
          {{ 'check_white.png' | file_img_url | img_tag }}
        {% endif %}
        {{ link.title | escape }}
      </a>
    </li>
  {% endfor %}
</ul>