<ul class="account-nav">
  {% for link in linklists.account-menu.links %}
    <li>
      <a href="{{ link.url }}">
        {% if link.title == "Home" %}
          {{ 'https://cdn.shopify.com/s/files/1/0401/9974/9789/files/Home_white.png?v=1592002592' | img_tag }}
        {% elsif link.title == "My Account" %}
          {{ 'https://cdn.shopify.com/s/files/1/0401/9974/9789/files/account_white.png?v=1592002848' | img_tag }}
        {% elsif link.title == "Shopping Cart" %}
          {{ 'https://cdn.shopify.com/s/files/1/0401/9974/9789/files/cart_white.png?v=1592002848' | img_tag }}
        {% elsif link.title == "Checkout" %}
          {{ 'https://cdn.shopify.com/s/files/1/0401/9974/9789/files/check_white.png?v=1592002848' | img_tag }}
        {% endif %}
        {{ link.title | escape }}
      </a>
    </li>
  {% endfor %}
  {% include 'gtranslate' %}
</ul>