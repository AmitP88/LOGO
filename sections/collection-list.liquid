<div class="collection-list">
  <!-- Tab Links -->
  <div class="tab">
    <button class="tablinks active" onClick="openCity(event, 'Fetured')" id="defaultOpen">Fetured</button>
    <button class="tablinks" onClick="openCity(event, 'New Itme')">New Itme</button>
    <button class="tablinks" onClick="openCity(event, 'Top Seller')">Top Seller</button>
    <button class="tablinks" onClick="openCity(event, 'Top Ratting')">Top Ratting</button>
  </div>

  <!-- Tab Content -->
  <div id="Fetured" class="tabcontent">
    <h3>Fetured</h3>
    <p>London is the capital city of England.</p>
  </div>
  
  <div id="New Itme" class="tabcontent">
    <h3>New Itme</h3>
    <p>Paris is the capital of France.</p> 
  </div>
  
  <div id="Top Seller" class="tabcontent">
    <h3>Top Seller</h3>
    <p>Tokyo is the capital of Japan.</p>
  </div>

  <div id="Top Ratting" class="tabcontent">
    <h3>Top Ratting</h3>
    <p>Tokyo is the capital of Japan.</p>
  </div>

  {{ 'tabs.js' | asset_url | script_tag }}

</div>




<script>
  // Get the element with id="defaultOpen" and click on it
  document.getElementById("defaultOpen").click();
</script>













