    </div>
  </div>



      
      
  <div class="tabcontent active" id="content1">
    <div class="tabs_container">
      {% for product in Featured_collection.products %}
        <div class="tab_product">
          <div class="product_photo">
            <a href="{{ product.url }}">
              <img src="{{ product.featured_image | img_url: 'large' }}" alt="product_image">            
            </a>
          </div>
          <div class="product_name_container">
            <p>{{ product.title }}</p>
          </div>
          <div class="price_and_rating">
            <div class="price">
              {% if product.compare_at_price %}
                <p class="previous_price">{{ product.compare_at_price | money_with_currency }}</p>
                <p class="current_price price_highlight">{{ product.price | money_with_currency }}</p>               
              {% else %}
                <p class="current_price">{{ product.price | money_with_currency }}</p>
              {% endif %}
            </div>
            <div class="rating">
              <div class="yotpo bottomLine"
                data-appkey="3lLBvLxtXto59qFkbJb0OW5hGtCQ7VU2CrOsDGgn"
                data-domain="{{shop.permanent_domain | escape }}"
                data-product-id="{{ product.id }}"
                data-product-models="{{ product.id }}"
                data-name="{{ product.title | escape }}"
                data-url="{{ shop.url }}{{ product.url }}"
                data-image-url="{{ product.featured_image | product_img_url: 'large' |replace: '?', '%3F' | replace: '&','%26'}}"
                data-description="{{ product.description | escape }}"
                data-bread-crumbs="{% for tag in product.tags %}{{ tag | escape }};{% endfor %}">
              </div>
            </div>
          </div>

          <div class="buttons_bar">
            <button class="add_to_cart">
              <img class="gray_icon" src="{{ 'https://cdn.shopify.com/s/files/1/0401/9974/9789/files/cart_gray.png?v=1593891089' }}" alt="gray_icon">
              <img class="green_icon" src="{{ 'https://cdn.shopify.com/s/files/1/0401/9974/9789/files/cart_green_button.png?v=1593916239' }}" alt="green_icon">
