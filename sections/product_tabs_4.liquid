<script>
  var currentTab = 1;
  $("#content1").show();

  var cur_tab = 1;
  $("#tab1").addClass("active");

  $(document).on('click', 'nav.multiTabs>a',
  function(){
      var TabId = $(this).attr('data-trigger');
      $('div#'+TabId+' ').show();
       
      // console.log("Current Tab: " + TabId);
      currentTab = parseInt(TabId.replace("content", ""));

      var tab = $(event.target).context;
      var tabId = tab.id;
      // console.log('tab: ', tab);
      // console.log('tab_id: ', tabId);

      cur_tab = parseInt(tabId.replace("tab", ""));
      // console.log('cur_tab: ', cur_tab);

      $(".tab").removeClass("active");
      $(tab).addClass("active");


      $('.tabcontent:not(#'+TabId+')').hide();

  });
   
  function next(){
      if(currentTab < 4){
          $(".tabcontent").hide();
          currentTab++;
          $("#content"+(currentTab)).show();
      }

      if(cur_tab < 4){
        $(".tab").removeClass("active");
        cur_tab++;
        // console.log(cur_tab);
        $("#tab"+(cur_tab)).addClass("active");
      }
  }
   
  function prev(){
      if(currentTab > 1){
          $(".tabcontent").hide();
          currentTab--;
          $("#content" + (currentTab)).show();
      }

      if(cur_tab > 1){
        $(".tab").removeClass("active");
        cur_tab--;
        // console.log(cur_tab);
        $("#tab"+(cur_tab)).addClass("active");
      }
  }
</script>